    "@iconify/json": "^2.2.110",
    "@iconify/vue": "^4.1.1",
    "@pixi/filter-adjustment": "^5.1.1",
    "@pixi/filter-advanced-bloom": "^5.1.1",
    "@pixi/filter-color-overlay": "^5.1.1",
    "@pixi/filter-outline": "^5.2.0",
    "@vee-validate/zod": "^4.11.3",
    "@vueuse/core": "^10.4.1",
    "@workadventure/tiled-map-type-guard": "^2.1.1",
    "astar-typescript": "^1.2.7",
    "convex": "^1.1.1",
    "gsap": "^3.12.2",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nanoid": "^4.0.2",
    "open-props": "^1.5.15",
    "pixi-viewport": "^5.0.2",
    "pixi.js": "^7.2.4",
    "vee-validate": "^4.11.3",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue3-pixi": "^0.8.1",
    "zipson": "^0.2.12",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/css-tree": "^2.3.1",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.5",
    "@unocss/postcss": "^0.53.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
