    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^10.0.0",
    "postcss-html": "^1.5.0",
    "postcss-nesting": "^12.0.0",
    "postcss-scrollbar": "^0.5.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.0.0",
    "stylelint": "^15.9.0",
    "stylelint-config-clean-order": "^5.0.1",
    "stylelint-config-html": "^1.1.0",
