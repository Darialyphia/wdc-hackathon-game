{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "tools/**/*.ts",
    "auto-imports.d.ts",
    "components.d.ts",
    "typed-router.d.ts",
    "src/assets/**/*.json"
  ],
  "exclude": ["src/**/__tests__/*", "src/assets/**/*.tsx"],
  "compilerOptions": {
    "composite": true,
    "resolveJsonModule": true
  }
}
